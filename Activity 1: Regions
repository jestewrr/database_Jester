mysql -h localhost -u root
show databases;
create database Jester;
use mydb;

CREATE TABLE Students (
idNumber INT(5) NOT NULL UNIQUE,
LastName VARCHAR(20) NOT NULL,
FirstName VARCHAR(20) NOT NULL,
MiddleName VARCHAR(20) NOT NULL,
Address VARCHAR(20) NOT NULL,
Age INT(3),
ContactNumber VARCHAR(13)
);

CREATE TABLE Regions (
Region_ID INT
Region_Name VARCHAR(25),
);

CREATE TABLE Countries (
Country_ID CHAR(2),
Country_Name VARCHAR(40),
Region_ID INT,
);

CREATE TABLE LOCATIONS (
Location_ID INT
STREET_ADDRESS VARCHAR(25),
POSTAL_CODE VARCHAR(12),
CITY VARCHAR(30),
STATE_PROVINCE VARCHAR(12),
COUNTRY_ID VARCHAR(2),
);

CREATE TABLE DEPARTMENTS (
DEPARTMENT_ID NUMBER,
DEPARTMENT_NAME VARCHAR2(20),
MANAGER_ID NUMBER,
LOCATION_ID NUMBER,
);

CREATE TABLE job_history (
EMPLOYEE_ID NUMBER,
START_DATE DATE,
END_DATE DATE,
JOB_ID,
DEPARTMENT_ID NUMBER,
);

CREATE TABLE employees (
EMPLOYEE_ID NUMBER,
FIRST_NAME VARCHAR2(20),
LAST_NAME VARCHAR2(25),
EMAIL VARCHAR2(25),
PHONE_NUMBER VARCHAR2(20),
HIRE_DATE DATE,
JOB_ID VARCHAR2(10),
SALARY NUMBER,
COMMISSION_POT NUMBER,
MANAGER_ID NUMBER,
DEPARTMENT_ID NUMBER,
);

CREATE TABLE Regions (
Region_ID Number
Region_Name VARCHAR(25),
AR (20) NOT NULL,
);

CREATE TABLE Countries (
Country_ID CHAR(2),
Country_Name VARCHAR2(40),
Region_ID Number,
);

CREATE TABLE LOCATIONS (
Location_ID Number
STREET_ADDRESS VARCHAR2(25),
POSTAL_CODE VARCHAR2(12),
CITY VARCHAR2(30),
STATE_PROVINCE VARCHAR2(12),
COUNTRY_ID CHAR(2),
);

CREATE TABLE DEPARTMENTS (
DEPARTMENT_ID NUMBER,
DEPARTMENT_NAME VARCHAR2(20),
MANAGER_ID NUMBER,
LOCATION_ID NUMBER,
);

CREATE TABLE job_history (
EMPLOYEE_ID NUMBER,
START_DATE DATE,
END_DATE DATE,
JOB_ID,
DEPARTMENT_ID NUMBER,
);

CREATE TABLE employees (
EMPLOYEE_ID NUMBER,
FIRST_NAME VARCHAR2(20),
LAST_NAME VARCHAR2(25),
EMAIL VARCHAR2(25),
PHONE_NUMBER VARCHAR2(20),
HIRE_DATE DATE,
JOB_ID VARCHAR2(10),
SALARY NUMBER,
COMMISSION_POT NUMBER,
MANAGER_ID NUMBER,
DEPARTMENT_ID NUMBER,
);

CREATE TABLE jobs (
JOB_ID VARCHAR(10),
JOB_TITLE VARCHAR(15),
MIN_SALARY INT,
MAX_SALARY INT
);

CREATE TABLE job_grades (
GRADE_LEVEL VARCHAR2(2)
LOWEST_SAL NUMBER,
HIGHEST_SAL NUMBER
);



